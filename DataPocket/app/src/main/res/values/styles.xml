<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="activity_add_genre" >
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="genre_title" >
        <item name="android:textSize">@dimen/genre_title_size</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="genre_message" >
        <item name="android:textSize">@dimen/default_size</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="activity_add_list">
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:textSize">@dimen/default_size</item>
    </style>

    <style name="custom_all_title_text" >
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="detail_bottom_parts" >
        <item name="android:background">#ffffff</item>
    </style>


    <!-- divider -->
    <style name="divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">#3F649A</item>
    </style>

    <!-- image box -->
    <style name="imageBox">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:layout_margin">20dp</item>
        <item name="android:background">@drawable/image_window</item>
    </style>
</resources>